<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity UserRate.
    -->
    <changeSet id="20230110175050-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="cargo_request_id"
                                 baseTableName="user_rate"
                                 constraintName="fk_user_rate__cargo_request_id"
                                 referencedColumnNames="id"

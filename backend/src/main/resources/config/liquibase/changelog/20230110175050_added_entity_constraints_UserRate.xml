        <addForeignKeyConstraint baseColumnNames="app_user_id"
                                 baseTableName="user_rate"
                                 constraintName="fk_user_rate__app_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="app_user"/>
    </changeSet>
</databaseChangeLog>

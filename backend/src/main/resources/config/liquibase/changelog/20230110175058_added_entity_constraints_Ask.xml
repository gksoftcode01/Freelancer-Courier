        <addForeignKeyConstraint baseColumnNames="cargo_request_id"
                                 baseTableName="ask"
                                 constraintName="fk_ask__cargo_request_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cargo_request"/>
    </changeSet>
</databaseChangeLog>
